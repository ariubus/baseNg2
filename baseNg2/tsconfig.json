{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "experimentalDecorators": true,
    "module": "commonjs",
    "outDir": "wwwroot/app",
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "typings"
  ]
}
